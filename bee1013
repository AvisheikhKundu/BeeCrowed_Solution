#include <stdio.h>

    int main()
    {
         int x, y, z, tmp;
         scanf("%d %d %d", &x, &y, &z);
         tmp = x;
         if(x < y || x < z){
             if(y < z){
             tmp = z;
             }
         else{
               tmp = y;
            }
        }
        printf("%d eh o maior\n", tmp);
        return 0;
    }              
